<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="senac.com.br.cademeulivro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icone_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icone_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id"/>

        <activity
            android:name=".activities.old.old_LoginActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        </activity>
        <activity android:name=".activities.old.old_MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.old.old_ObraDetalhadaActivity"/>
        <activity android:name=".activities.old.old_ObraCadastroActivity">
        </activity>
    </application>

</manifest>